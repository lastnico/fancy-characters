{
  "name": "Fancy Characters",
  "version": "1.0",
  "description": "Get a list of Fancy Characters",
  "browser_action": {
    "default_icon": "icon.png",
	"popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*", "file://*/*" ],
      "js": ["page.js"],
      "all_frames" : true
    }
  ],
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "file://*/*"
  ]
}